# LinkedIn Automation Tool

LINKEDIN_EMAIL=your_email@example.com
LINKEDIN_PASSWORD=your_password_here

# Database
DATABASE_PATH=./data/linkedin_automation.db

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# Proxy Settings (optional)
USE_PROXY=false
PROXY_LIST=proxy1:port,proxy2:port,proxy3:port

# Browser Settings
HEADLESS_MODE=true
BROWSER_TIMEOUT=30000
NAVIGATION_TIMEOUT=30000

# Job Scraping Settings
SCRAPE_INTERVAL_MINUTES=10
MAX_JOBS_PER_SCRAPE=50
JOB_SEARCH_KEYWORDS=software engineer,full stack,backend,frontend
JOB_LOCATIONS=United States,Remote
JOB_TYPES=Full-time,Contract

# Auto Application Settings
AUTO_APPLY_ENABLED=false
MAX_APPLICATIONS_PER_DAY=20
APPLICATION_DELAY_MS=5000

# Rate Limiting
REQUESTS_PER_MINUTE=30
BURST_LIMIT=5

# Server Settings
PORT=3000
API_KEY=your_secure_api_key_here
